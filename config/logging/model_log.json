{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "simple"
    },
    "file": {
      "class": "logging.FileHandler",
      "level": "INFO",
      "formatter": "simple",
      "filename":"LOGS/model.log"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "console",
      "file"
    ]
  }
}